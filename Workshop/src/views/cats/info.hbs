<form action="/cats/{{cat.id}}/changeInfo" method="POST" class="cat-form"></form>
            <h2>Add Cat</h2>
            <label for="name">Name</label>
            <input name="name" type="text" id="name" value={{cat.name}}>
            <label for="description">Description</label>
            <textarea name="description" id="description" >{{cat.description}}</textarea>
            <label for="image">Image</label>
            <textarea name="image" id="imageUrl"  >{{cat.imageUrl}}</textarea>
            <label for="group">Breed</label>
            <select name="breed" id="group">
        {{#each breeds}}
            <option value="breed">{{breed}}</option>
        {{/each}}
            </select>
            <button type="submit">Add Cat</button>
</form>